---
import { contacts } from "../constants/contacts";
import HomeContactItem from "./HomeContactItem.astro";
---

<ul class={`grid grid-cols-1 md:grid-cols-2 gap-4 p-6`}>
  {
    contacts.map(({ link, icon, value }) => (
      <HomeContactItem link={link} icon={icon} value={value} />
    ))
  }
</ul>
